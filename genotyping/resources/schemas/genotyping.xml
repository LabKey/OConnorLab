<ns:tables xmlns:ns="http://labkey.org/data/xml">
  <ns:table tableName="Runs" tableDbType="TABLE">
    <ns:titleColumn>RowId</ns:titleColumn>
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:url>/genotyping/run.view?run=${RowId}</ns:url>
        <ns:columnTitle>Run</ns:columnTitle>
      </ns:column>
      <ns:column columnName="MetaDataId"/>
      <ns:column columnName="Container"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="Path"/>
      <ns:column columnName="FileName"/>
      <ns:column columnName="Status"/>
      <ns:column columnName="Platform"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Analyses" tableDbType="TABLE">
    <ns:titleColumn>RowId</ns:titleColumn>
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:url>/genotyping/analysis.view?analysis=${RowId}</ns:url>
        <ns:columnTitle>Analysis</ns:columnTitle>
      </ns:column>
      <ns:column columnName="Run">
        <ns:url>/genotyping/run.view?run=${Run}</ns:url>
      </ns:column>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="Description"/>
      <ns:column columnName="Path"/>
      <ns:column columnName="FileName"/>
      <ns:column columnName="SequenceDictionary"/>
      <ns:column columnName="SequencesView"/>
      <ns:column columnName="Status"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Matches" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Analysis">
        <ns:url>/genotyping/analysis.view?analysis=${Analysis}</ns:url>
      </ns:column>
      <ns:column columnName="SampleId"/>
      <ns:column columnName="Reads">
        <ns:url>/genotyping/matchReads.view?analysis=${Analysis}&amp;match=${RowId}</ns:url>
      </ns:column>
      <ns:column columnName="Percent">
        <ns:formatString>0.0%</ns:formatString>
      </ns:column>
      <ns:column columnName="AverageLength">
        <ns:formatString>0.0</ns:formatString>
      </ns:column>
      <ns:column columnName="PosReads"/>
      <ns:column columnName="NegReads"/>
      <ns:column columnName="PosExtReads"/>
      <ns:column columnName="NegExtReads">
        <ns:displayWidth>65</ns:displayWidth>
      </ns:column>
      <ns:column columnName="ParentId"/>
      <ns:column columnName="Alleles" wrappedColumnName="RowId">
        <ns:isUnselectable>true</ns:isUnselectable>
        <ns:fk>
          <ns:fkDbSchema>genotyping</ns:fkDbSchema>
          <ns:fkTable>AllelesJunction</ns:fkTable>
          <ns:fkColumnName>MatchId</ns:fkColumnName>
          <ns:fkMultiValued>junction</ns:fkMultiValued>
          <ns:fkJunctionLookup>SequenceId</ns:fkJunctionLookup>
        </ns:fk>
      </ns:column>
    </ns:columns>
  </ns:table>
  <ns:table tableName="ReadsJunction" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="MatchId"/>
      <ns:column columnName="ReadId"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="AllelesJunction" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="Analysis"/>
      <ns:column columnName="MatchId"/>
      <ns:column columnName="SequenceId"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="AnalysisSamples" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="Analysis"/>
      <ns:column columnName="SampleId"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Reads" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Run"/>
      <ns:column columnName="Name"/>
      <ns:column columnName="Mid">
        <ns:isHidden>true</ns:isHidden>
      </ns:column>          
      <ns:column columnName="SampleId"/>
      <ns:column columnName="Sequence"/>
      <ns:column columnName="Quality"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Dictionaries" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId"/>
      <ns:column columnName="Container"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Sequences" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Dictionary"/>
      <ns:column columnName="Uid"/>
      <ns:column columnName="AlleleName">
        <!--ns:url>/genotyping/sequence.view?sequence=${RowId}</ns:url-->
      </ns:column>
      <ns:column columnName="Initials"/>
      <ns:column columnName="GenbankId"/>
      <ns:column columnName="ExptNumber"/>
      <ns:column columnName="Comments"/>
      <ns:column columnName="Locus"/>
      <ns:column columnName="Species"/>
      <ns:column columnName="Origin"/>
      <ns:column columnName="Sequence"/>
      <ns:column columnName="Previousname"/>
      <ns:column columnName="LastEdit"/>
      <ns:column columnName="Version"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Translation"/>
      <ns:column columnName="Type"/>
      <ns:column columnName="IpdAccession"/>
      <ns:column columnName="Reference"/>
      <ns:column columnName="Region"/>
      <ns:column columnName="Id"/>
      <ns:column columnName="Variant"/>
      <ns:column columnName="UploadId"/>
      <ns:column columnName="FullLength"/>
      <ns:column columnName="AlleleFamily"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="SequenceFiles" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Run"/>
      <ns:column columnName="DataId">
          <ns:fk>
              <ns:fkDbSchema>exp</ns:fkDbSchema>
              <ns:fkTable>data</ns:fkTable>
              <ns:fkColumnName>RowId</ns:fkColumnName>
          </ns:fk>
      </ns:column>
      <ns:column columnName="SampleId"/>
      <ns:column columnName="ReadCount"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="IlluminaTemplates" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="Name"/>
      <ns:column columnName="Json"/>
      <ns:column columnName="Editable"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Modified"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Haplotype" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Container"/>
      <ns:column columnName="Name"/>
      <ns:column columnName="Type"/>
      <ns:column columnName="Lsid"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Modified"/>
      <ns:column columnName="SpeciesId">
        <ns:fk>
            <ns:fkDbSchema>genotyping</ns:fkDbSchema>
            <ns:fkTable>Species</ns:fkTable>
            <ns:fkColumnName>RowId</ns:fkColumnName>
        </ns:fk>
      </ns:column>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Animal" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Container"/>
      <ns:column columnName="LabAnimalId"/>
      <ns:column columnName="ClientAnimalId"/>
      <ns:column columnName="Lsid"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Modified"/>
      <ns:column columnName="SpeciesId">
        <ns:fk>
          <ns:fkDbSchema>genotyping</ns:fkDbSchema>
          <ns:fkTable>Species</ns:fkTable>
          <ns:fkColumnName>RowId</ns:fkColumnName>
        </ns:fk>
      </ns:column>
    </ns:columns>
  </ns:table>
    <ns:table tableName="Species" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="RowId">
          <ns:isHidden>true</ns:isHidden>
          <ns:isKeyField>true</ns:isKeyField>
        </ns:column>
        <ns:column columnName="Name"/>
      </ns:columns>
    </ns:table>
  <ns:table tableName="AnimalAnalysis" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="AnimalId">
        <ns:columnTitle>Animal</ns:columnTitle>
      </ns:column>
      <ns:column columnName="RunId">
        <ns:columnTitle>Run</ns:columnTitle>
      </ns:column>
      <ns:column columnName="TotalReads"/>
      <ns:column columnName="IdentifiedReads"/>
      <ns:column columnName="Enabled"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Modified"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="AnimalHaplotypeAssignment" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="AnimalAnalysisId">
          <ns:columnTitle>AnimalAnalysis</ns:columnTitle>
      </ns:column>
      <ns:column columnName="HaplotypeId">
          <ns:columnTitle>Haplotype</ns:columnTitle>
      </ns:column>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Modified"/>
    </ns:columns>
  </ns:table>
</ns:tables>