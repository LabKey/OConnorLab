<ns:tables xmlns:ns="http://labkey.org/data/xml">
    <ns:table tableName="AggregatedAnalysisResults" tableDbType="NOT_IN_DB">
        <ns:columns>
            <ns:column columnName="AnimalId">
                <ns:columnTitle>Animal</ns:columnTitle>
            </ns:column>
            <ns:column columnName="TotalReads">
                <ns:columnTitle>Total Reads</ns:columnTitle>
            </ns:column>
            <ns:column columnName="TotalIdentifiedReads">
                <ns:columnTitle>Total Identified Reads</ns:columnTitle>
            </ns:column>
            <ns:column columnName="TotalPercentUnknown">
                <ns:columnTitle>Total % Unknown</ns:columnTitle>
                <ns:formatString>0.0'%'</ns:formatString>
            </ns:column>
            <ns:column columnName="ConcatenatedHaplotypes">
                <ns:columnTitle>Concatenated Haplotypes</ns:columnTitle>
            </ns:column>
            <ns:column columnName="InconsistentAssignments">
                <ns:conditionalFormats>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter value="true" operator="eq"/>
                        </ns:filters>
                        <ns:backgroundColor>FF8080</ns:backgroundColor>
                    </ns:conditionalFormat>
                </ns:conditionalFormats>
            </ns:column>
            <ns:column columnName="mhcAHaplotype1">
                <ns:columnTitle>MHC-A Haplotype 1</ns:columnTitle>
            </ns:column>
            <ns:column columnName="mhcAHaplotype2">
                <ns:columnTitle>MHC-A Haplotype 2</ns:columnTitle>
            </ns:column>
            <ns:column columnName="mhcBHaplotype1">
                <ns:columnTitle>MHC-B Haplotype 1</ns:columnTitle>
            </ns:column>
            <ns:column columnName="mhcBHaplotype2">
                <ns:columnTitle>MHC-B Haplotype 2</ns:columnTitle>
            </ns:column>
        </ns:columns>
    </ns:table>
</ns:tables>
