<customView xmlns="http://labkey.org/data/xml/queryCustomView">
<!--

Defines a custom view on the active_grants query. Used to generate pulldown menu for popup that displays when reordering products.

-->

   <columns>
	<column name="id"/>
	<column name="grantDescription"/>
   </columns>
   <filters>
       <filter column="status" operator="eq" value="3"/>
   </filters>
</customView>