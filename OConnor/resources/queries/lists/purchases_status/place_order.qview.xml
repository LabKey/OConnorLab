<customView xmlns="http://labkey.org/data/xml/queryCustomView">
<!--

view for purchases_placeOrders.html page. uses this query so that the 'placedBy' and 'place Date' fields from the audit table display

-->

   <columns>
   <column name="Key"/>
	<column name="item"/>
	<column name="itemNumber"/>
	<column name="quantity"/>
	<column name="unit"/>
	<column name="price"/>
	<column name="vendor/vendor"/>
	<column name="vendor/phone"/>
	<column name="vendor/email"/>
	<column name="vendor/po"/>
	<column name="placedBy"/>
	<column name="placeDate"/>
	<column name="grant/id"/>
	<column name="grant/department"/>
	<column name="comment"/>
   </columns>
   <filters>
       <filter column="status" operator="eq" value="1"/>
   </filters>
</customView>

										
